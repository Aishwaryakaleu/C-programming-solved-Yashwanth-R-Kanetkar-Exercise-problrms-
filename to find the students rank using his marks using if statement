#include<stdio.h>
int main() 
{  
int   m1, m2, m3, m4, m5, per ; 
printf ( "Enter values of m1 = ");
scanf("%d", &m1);
printf ( "Enter values of m2 = ");
scanf("%d", &m2);
printf ( "Enter values of m3 = ");
scanf("%d", &m3);
printf ( "Enter values of m4 = ");
scanf("%d", &m4);
printf ( "Enter values of m5 = ");
scanf("%d", &m5);

per = ( m1 + m2 + m3 + m4 + m5 ) / 5 ; 
if ( per >= 60 )
printf ( "First division ") ;
else 
{   
if ( per >= 50 )    
printf ( "Second division" ) ;   
else   
{    
if ( per >= 40 )     
printf ( "Third division" ) ;    
else     
printf ( "Fail" ) ;   
}   

} 

} 
